{
  "version": 2,
  "buildCommand": "cd frontend && npm install && npm run build -- --configuration=production",
  "outputDirectory": "frontend/dist/angular-10-17-updated-boilerplate",
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/(.*\\.(js|css|png|jpg|jpeg|gif|ico|json|svg|woff|woff2|ttf|eot))",
      "dest": "/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
} 